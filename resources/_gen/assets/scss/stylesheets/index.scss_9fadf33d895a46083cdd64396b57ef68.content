@media print {
  /**
	 * @see http://www.alistapart.com/articles/goingtoprint/
	 * @see http://www.merttol.com/articles/css/too-light-for-print.html
	 */
  * {
    text-shadow: none !important;
    color: black !important;
    /* Black prints faster: h5bp.com/s */
    background: transparent !important; }
  body {
    font: 12pt/1.33333 'Palatino Linotype', Palatino, 'Times New Roman', Times, serif;
    color: black;
    background: white none; }
  a,
  a:link,
  a:visited {
    font-weight: bold;
    text-decoration: underline;
    color: #444444 !important;
    background: transparent; }
  a[href]::after {
    font-style: italic;
    word-wrap: break-word;
    white-space: pre-wrap;
    color: #333333;
    content: " (" attr(href) ") "; }
  /**
	* Don't show links for images, or javascript/internal links
	*/
  a[href^='javascript:']::after,
  a[href^='#']::after {
    content: ''; }
  abbr[title]::after {
    font-style: italic;
    content: " (" attr(title) ")"; }
  pre,
  blockquote {
    page-break-inside: avoid;
    border: 1px solid #999999; }
  /**
	 * @see css-discuss.incutio.com/wiki/Printing_Tables
	 */
  thead {
    display: table-header-group; }
  tr,
  img {
    page-break-inside: avoid; }
  img {
    max-width: 100% !important;
    vertical-align: bottom; }
  @page {
    margin: 0.5cm; }
  p,
  h2,
  h3 {
    orphans: 3;
    widows: 3; }
  h1,
  h2,
  h3,
  h4,
  h5,
  h6,
  legend,
  caption {
    page-break-after: avoid;
    page-break-inside: never; }
  [role='main'] {
    float: none !important;
    width: auto;
    padding: 0;
    margin: 0 5%;
    border: 0; }
  nav,
  .access,
  .secondary {
    display: none; } }

